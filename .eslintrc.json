{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "linebreak-style": "off",
    "import/no-named-as-default": "off",
    "import/no-unresolved": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-use-before-define": "off",
    "import/extensions": "off"
  }
}